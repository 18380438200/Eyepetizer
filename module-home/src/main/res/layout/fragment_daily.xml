    <data>
        <variable
            name="viewModel"
            type="com.example.module_home.module.home.DailyViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
